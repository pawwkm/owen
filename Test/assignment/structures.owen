// expect 0

namespace Assignments

ctfe main()

structure Rectangle
    Point a
    Point b
end

structure Point
    I32 x
    I32 y
end

function main
    output I32

    p = Point
        x = 1,
        y = 2
    end

    assert p.x == 1
    assert p.y == 2

    p.x = 3
    assert p.x == 3

    r = Rectangle
        a = p,
        b = Point
            x = 4,
            y = 5
        end
    end

    assert r.a.x == 3
    assert r.a.y == 2
    assert r.b.x == 4
    assert r.b.y == 5

    return 0
end