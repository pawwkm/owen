namespace Precedence

function main
    output I32
    
    assert true || true && false
    assert true && false == false
    assert true && false != true
    assert true && 0i32 <= 1
    assert true && 1i32 >= 0
    assert true && 0i32 < 1
    assert true && 1i32 > 0
    assert true && 1i32 + 1 == 2
    assert true && 1i32 - 1 == 0
    assert true && 1i32 | 2 == 3
    assert true && 1i32 ^ 2 == 3
    assert 1i32 + 2 * 3 == 7
    assert 2i32 + 4 / 2 == 4
    assert 1i32 + 5 % 2 == 2
    assert 1i32 + 6 & 3 == 3
    assert 1i32 + 1 << 1 == 3
    assert 1i32 + 8 >> 2 == 3

    return 0
end